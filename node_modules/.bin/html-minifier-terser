#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Laptop/Downloads/weather-dashboard/node_modules/html-minifier-terser/node_modules:/mnt/c/Users/Laptop/Downloads/weather-dashboard/node_modules:/mnt/c/Users/Laptop/Downloads/node_modules:/mnt/c/Users/Laptop/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/Laptop/Downloads/weather-dashboard/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Laptop/Downloads/weather-dashboard/node_modules/html-minifier-terser/node_modules:/mnt/c/Users/Laptop/Downloads/weather-dashboard/node_modules:/mnt/c/Users/Laptop/Downloads/node_modules:/mnt/c/Users/Laptop/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/Laptop/Downloads/weather-dashboard/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../html-minifier-terser/cli.js" "$@"
else
  exec node  "$basedir/../html-minifier-terser/cli.js" "$@"
fi
